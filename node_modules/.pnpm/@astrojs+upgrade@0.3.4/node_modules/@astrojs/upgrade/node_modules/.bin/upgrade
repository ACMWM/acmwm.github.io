#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robert/devel/gh/acm-next/node_modules/.pnpm/@astrojs+upgrade@0.3.4/node_modules/@astrojs/upgrade/node_modules:/Users/robert/devel/gh/acm-next/node_modules/.pnpm/@astrojs+upgrade@0.3.4/node_modules/@astrojs/node_modules:/Users/robert/devel/gh/acm-next/node_modules/.pnpm/@astrojs+upgrade@0.3.4/node_modules:/Users/robert/devel/gh/acm-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/robert/devel/gh/acm-next/node_modules/.pnpm/@astrojs+upgrade@0.3.4/node_modules/@astrojs/upgrade/node_modules:/Users/robert/devel/gh/acm-next/node_modules/.pnpm/@astrojs+upgrade@0.3.4/node_modules/@astrojs/node_modules:/Users/robert/devel/gh/acm-next/node_modules/.pnpm/@astrojs+upgrade@0.3.4/node_modules:/Users/robert/devel/gh/acm-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../upgrade.mjs" "$@"
else
  exec node  "$basedir/../../upgrade.mjs" "$@"
fi
